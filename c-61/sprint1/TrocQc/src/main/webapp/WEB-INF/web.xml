<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>InscriptionUsagerServlet</servlet-name>
  	<display-name>InscriptionUsagerServlet</display-name>
  	<description></description>
  	<servlet-class>com.TrocQc.servlets.InscriptionUsagerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>getUserAvatar</servlet-name>
  	<display-name>getUserAvatar</display-name>
  	<description></description>
  	<servlet-class>com.TrocQc.servlets.getUserAvatar</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>InscriptionUsagerServlet</servlet-name>
  	<url-pattern>/InscriptionUsagerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>getUserAvatar</servlet-name>
  	<url-pattern>/getUserAvatar</url-pattern>
  </servlet-mapping>
</web-app>